      "filter": null,
      "sortOrder": [
        { "column": "seo_triageCategory", "direction": "Ascending" }
      ]
    },
    {
      "schemaName": "seo_TransportTracking",
      "displayName": "Transport Tracking",
      "entity": "seo_PatientRecord",
      "description": "Patients that have been transported, sorted by transport start time descending. No PHI columns exposed.",
